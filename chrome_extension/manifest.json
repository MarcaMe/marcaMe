{
  "name": "MarcaMe",
  "version": "1.2",
  "description": "my chrome extension",
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "cookies"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "MarcaMe",
    "default_popup": "./Popup/popup.html"
  },
  "web_accessible_resources": [
    "./Dependencies/semantic.min.css", "./Dependencies/semantic.min.js"
  ],
  "background": {
    "scripts": ["./Dependencies/jquery.min.js"],
    "persistent": true
  },
  "manifest_version": 2
}
